<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../partials/head'); %>
</head>

<body class="light">
    <%- include('../partials/header', {page_name: 'dashboard' } ); %>

        <div class="row row-eq-height my-3">

            <div class="col-lg-3 col-sm-4 col-xs-12 mb-3">
                <div class="counter-box p-40 white shadow2 r-5">
                    <div class="float-right">
                        <span class="icon icon-people_outline s-48"></span>
                    </div>
                    <div class="sc-counter s-36">
                        <%= total_users %>
                    </div>
                    <h6 class="counter-title">Total Events</h6>
                </div>
            </div>

            <div class="col-lg-3 col-sm-4 col-xs-12 mb-3">
                <div class="counter-box p-40 white shadow2 r-5">
                    <div class="float-right">
                        <span class="icon icon-people_outline s-48"></span>
                    </div>
                    <div class="sc-counter s-36">0</div>
                    <h6 class="counter-title">Total Members</h6>
                </div>
            </div>

        </div>

        <%- include('../partials/footer'); %>
</body>

</html>