<section class="sidebar">
    <div class="xw-80px mt-3 mb-3 xml-3 text-center">
        <a href="/">
            <img src="/admin/assets/img/logo/logo_small.png" alt="E-Team Building" style="height:48px;">
            <h2 class="mt-3">E-Team Building</h2>
        </a>
    </div>
    <ul class="sidebar-menu hover-dark">
        <li class="header"><strong>MAIN NAVIGATION</strong></li>
        <li class="<%= page_name === 'dashboard' ? 'nav-active' : '' %>">
            <a href="/admin/dashboard">
                <i class="icon icon-dashboard2 s-18"></i> 
                <span>Dashboard</span>
            </a>
        </li>

        <% if ( permissions.includes('perm-client-member') ) { %>
            <li class="treeview <%= (['member'].includes(page_name)) ? 'active' : '' %>">
                <a href="#">
                    <i class="icon icon-dashboard2 light-green-text s-18"></i>
                    Client Member Management
                    <i class="icon icon-angle-left s-18 pull-right"></i>
                </a>
                <ul class="treeview-menu">
                    
                    <li class="<%= page_name === 'customer' ? 'nav-active' : '' %>">
                        <a href="/admin/client-members">
                            <i class="icon icon-circle-o"></i>Members
                        </a>
                    </li>
                    
                </ul>
            </li>
        <% } %>


        <% if ( permissions.includes('perm-admin-user') || permissions.includes('perm-role') ) { %>
        <li class="treeview <%= (['role', 'admin_user'].includes(page_name)) ? 'active' : '' %>">
            <a href="#">
                <i class="icon icon-dashboard2 light-green-text s-18"></i>
                Admin Users Management
                <i class="icon icon-angle-left s-18 pull-right"></i>
            </a>
            <ul class="treeview-menu">
                
                <% if ( permissions.includes('perm-admin-user') ) { %>
                <li class="<%= page_name === 'admin_user' ? 'nav-active' : '' %>">
                    <a href="/admin/users">
                        <i class="icon icon-circle-o"></i>Admin Users
                    </a>
                </li>
                <% } %>
                
                <% if ( permissions.includes('perm-role') ) { %>
                <li class="<%= page_name === 'role' ? 'nav-active' : '' %>">
                    <a href="/admin/roles">
                        <i class="icon icon-circle-o"></i>Roles Management
                    </a>
                </li>
                <% } %>
        
            </ul>
        </li>
        <% } %>

        <li style="height: 200px;">

        </li>

    </ul>
</section>